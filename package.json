{
  "name": "@peterjokumsen/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky || true",
    "update-peer-dependencies": "node update-peer-dependencies.js",
    "mega-lint": "npx mega-linter-runner --flavor javascript",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "pre-commit:write": "nx format:write && pnpm run lint:uncommitted",
    "pre-push:check": "nx affected --base=origin/main -t lint test build --parallel=5",
    "swa-local-app-only": "swa start http://localhost:4200",
    "serve:blog": "nx run blog:serve",
    "serve:bu-jo": "nx run bullet-journal:serve",
    "build:blog": "nx run blog:build",
    "build:bu-jo": "nx run bullet-journal:build",
    "e2e:blog": "nx run blog-e2e:e2e-ci",
    "e2e:bu-jo": "nx run bullet-journal-e2e:e2e-ci",
    "e2e:blog:local": "nx run blog-e2e:e2e",
    "e2e:bu-jo:local": "nx run bullet-journal-e2e:e2e",
    "swa:blog": "concurrently --kill-others \"pnpm run serve:blog\" \"pnpm run swa-local-app-only\"",
    "swa:bu-jo": "concurrently --kill-others \"pnpm run serve:bu-jo\" \"pnpm run swa-local-app-only\"",
    "build:all": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build --base=origin/main",
    "lint:all": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint --base=origin/main --parallel=5",
    "lint:uncommitted": "nx affected --target=lint --uncommitted --parallel=5",
    "test:all": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test --base=origin/main",
    "test:uncommitted": "nx affected --target=test --uncommitted"
  },
  "packageManager": "pnpm@9.15.2",
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.7",
    "@angular/cdk": "18.2.6",
    "@angular/common": "18.2.7",
    "@angular/compiler": "18.2.7",
    "@angular/core": "18.2.7",
    "@angular/forms": "18.2.7",
    "@angular/material": "18.2.6",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "18.2.7",
    "@angular/platform-server": "18.2.7",
    "@angular/router": "18.2.7",
    "@angular/service-worker": "18.2.7",
    "@angular/ssr": "18.2.7",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@nx/angular": "20.0.6",
    "express": "^4.20.0",
    "highlight.js": "^11.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.7",
    "@angular-devkit/core": "18.2.7",
    "@angular-devkit/schematics": "18.2.7",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "~18.2.0",
    "@angular/compiler-cli": "18.2.7",
    "@angular/language-service": "18.2.7",
    "@angular/pwa": "18.2.7",
    "@azure/static-web-apps-cli": "^2.0.1",
    "@nx/devkit": "20.0.6",
    "@nx/esbuild": "20.0.6",
    "@nx/eslint": "20.0.6",
    "@nx/eslint-plugin": "20.0.6",
    "@nx/jest": "20.0.6",
    "@nx/js": "20.0.6",
    "@nx/playwright": "20.0.6",
    "@nx/web": "20.0.6",
    "@nx/workspace": "20.0.6",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "18.2.7",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "4.17.14",
    "@types/jest": "29.5.13",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@typescript-eslint/utils": "7.18.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.0.1",
    "esbuild": "^0.19.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "husky": "^9.0.11",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.4.1",
    "jest-preset-angular": "14.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "ng-mocks": "^14.12.1",
    "ng-packagr": "18.2.1",
    "nx": "20.0.6",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.5.4"
  }
}
